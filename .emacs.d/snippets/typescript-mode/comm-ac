export const { execute: ${1:$(s-lower-camel-case yas-text)}, completed: ${1:$(s-lower-camel-case yas-text)}Completed, failed: ${1:$(s-lower-camel-case yas-text)}Fail } =
  makeCommunicationActionCreators<NS.I${1:ActionName}, NS.I$1Completed, NS.I$1Failed>(
    '${1:$(yas:get-action-type yas-text)}', '${1:$(yas:get-action-type yas-text)}_COMPLETED', '${1:$(yas:get-action-type yas-text)}_FAILED',
  );